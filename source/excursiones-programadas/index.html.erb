---
layout: layout
---
<div class="inner-banner style-6 background-block" style="background-image: url(img/gallery/bg_3.jpg);">
	<img class="center-image" src="/images/inner_banner_1.jpg" alt="" style="display: none;">
	<div class="vertical-align">
		<div class="container">
			<div class="row">
				<div class="col-xs-12 col-md-8 col-md-offset-2">
		  			<ul class="banner-breadcrumb color-white clearfix">
		  				<li><a class="link-blue-2" href="/">Inicio</a> /</li>
		  				<li><span>Excursiones</span></li>
		  			</ul>
		  			<h2 class="color-white">Excursiones Progamadas</h2>
  				</div>
			</div>
		</div>
	</div>
</div>

<div class="main-wraper padd-70-70">
	<div class="container">
		<div class="filter-content row" style="position: relative; height: 365.234px;">
			<div class="grid-sizer col-mob-12 col-xs-6 col-sm-2"></div>
			<% dato.excursiones_programadas.each do |item| %>
					<div class="item gal-item style-3 col-mob-12 col-xs-6 col-sm-4" style="position: absolute; left: 0px; top: 0px;">
						<a class="black-hover" href="/excursiones-programadas/<%= imagen_slug(item.titulo)%>.html">
							<div class="gal-item-icon">
                <%= image_tag item.imagen.file.width(360).height(215).fit('crop').to_url %>
								<div class="tour-layer delay-1"></div>
								<div class="vertical-align">
								</div>
							</div>
							<div class="gal-item-desc delay-1">
								<h4><b><%= item.titulo %></b></h4>
							</div>
						</a>
					</div>
			<% end %>

		</div>
	</div>
</div>
